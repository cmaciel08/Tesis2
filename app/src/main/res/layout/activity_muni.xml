<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#E4E5DD"
    android:layout_height="match_parent"
    tools:context="android.prueba.david.tesis.MuniActivity.MuniActivity">

    <ImageButton
        android:id="@+id/imageButtonMaps3"
        android:layout_width="50dp"
        android:layout_height="46dp"
        android:contentDescription=""
        android:onClick="verMapa"
        app:srcCompat="@drawable/ic_map2"
        android:background="@null"
        tools:ignore="ContentDescription,VectorDrawableCompat"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/listViewMuni"
        android:layout_marginRight="16dp"
        app:layout_constraintVertical_bias="0.769" />

    <ListView
        android:id="@+id/listViewMuni"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="5dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="17dp"
        app:layout_constraintRight_toRightOf="@+id/imageButtonMaps3"
        android:layout_marginTop="65dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="17dp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="312dp"
        android:layout_height="60dp"
        android:layout_marginTop="7dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="16dp">

        <ImageButton
            android:id="@+id/imageButtonContacto2"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginEnd="1dp"
            android:layout_marginRight="23dp"
            android:layout_weight="1"
            android:background="@null"
            app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
            app:layout_constraintRight_toRightOf="@+id/relativeLayout"
            app:layout_constraintTop_toTopOf="@+id/imageButtonInfo2"
            app:srcCompat="@drawable/ic_phone_book"
            tools:ignore="ContentDescription"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />


        <ImageButton
            android:id="@+id/imageButtonInfo2"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginEnd="2dp"
            android:layout_marginLeft="23dp"
            android:layout_marginRight="1dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="3dp"
            android:layout_weight="1"
            android:background="@null"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imageButtonContacto2"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_folder"
            tools:ignore="ContentDescription"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />



    </LinearLayout>
</android.support.constraint.ConstraintLayout>
