<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="android.prueba.david.tesis.PjudicialActivity.Pjudicial2Activity">


    <ImageButton
        android:id="@+id/imageButtonMaps2"
        android:layout_width="52dp"
        android:layout_height="0dp"
        android:contentDescription=""
        android:onClick="verMapa"
        app:srcCompat="@drawable/ic_map2"
        tools:ignore="ContentDescription,VectorDrawableCompat"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="10dp"
        tools:layout_editor_absoluteX="308dp"
        android:layout_marginBottom="-10dp"
        android:layout_marginRight="10dp" />

    <ListView
        android:id="@+id/listViewPjudicial2"
        android:layout_width="340dp"
        android:layout_height="383dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.475"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">


    </ListView>

</android.support.constraint.ConstraintLayout>
